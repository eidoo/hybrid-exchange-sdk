{
    "env": {
        "jest": true,
        "node": true
    },
    "extends": "airbnb-base",
    "rules": {
        "max-len": [
            "error",
            120
        ],
        "no-await-in-loop": "off",
        "no-shadow": "off",
        "no-underscore-dangle": "off",
        "object-curly-newline": [
            "error",
            {
                "consistent": true
            }
        ],
        "semi": [
            "error",
            "never"
        ],
        "space-before-function-paren": "off"
    }
}